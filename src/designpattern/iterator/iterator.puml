@startuml
package iterator{
    +interface Aggregate{
        +{abstract} iterator()
    }

    +interface Iterator{
        +{abstract} hasNext()
        +{abstract} next()
    }

    +class BookShelf{
        books
            last
            getBookAt()
            appendBook()
            getLength()
            iterator()
    }

    +class BookShelfIterator{
        bookShelf
            index
            hasNext()
            next()
    }
    +class Book{
        name
            getName()
    }

    Aggregate -> Iterator : Creates >
        BookShelf <-o BookShelfIterator
         Aggregate <|.. BookShelf
         Iterator <|.. BookShelfIterator
        BookShelf o--> Book
}
@enduml
